

public class Movie {
/*
 * id:integer (primary key)
title:varchar(100) 
year:integer 
director:varchar(100) 
banner_url:varchar(200) 
trailer_url:varchar(200) 
 */
	private String title;

	private int age;
	
	private int id;

	private String type;
	
	public Movie(){
		
	}
	
	public Movie(String title, int id, int age,String type) {
		this.title = title;
		this.age = age;
		this.id  = id;
		this.type = type;
		
	}
	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return title;
	}

	public void setName(String name) {
		this.title = name;
	}


	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}	
	
	
	public String toString() {
		StringBuffer sb = new StringBuffer();
		sb.append("Employee Details - ");
		sb.append("Name:" + getName());
		sb.append(", ");
		sb.append("Type:" + getType());
		sb.append(", ");
		sb.append("Id:" + getId());
		sb.append(", ");
		sb.append("Age:" + getAge());
		sb.append(".");
		
		return sb.toString();
	}
}