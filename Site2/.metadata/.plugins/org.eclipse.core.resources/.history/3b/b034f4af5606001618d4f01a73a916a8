var name = document.getElementById("dbSearchBox");
var title = document.getElementById("dbSearchBox");
var director = document.getElementById("dbSearchBox");
var year = document.getElementById("dbSearchBox");
function submitSearch(){
	var queryString = "?";
	if(name != null)
		if(name.value != "")
			queryString += "name="+name.value + "&";
	if(title != null)
		if(title.value != "")
			queryString += "title="+title.value + "&";
	if(director != null)
		if(director.value != "")
			queryString += "director="+director.value + "&";
	if(year != null)
		if(year.value != "")
			queryString += "year="+year.value;
	document.location.search = queryString
	
};